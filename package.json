{
  "name": "can-dom-mutate",
  "description": "Dispatch and listen for DOM mutations",
  "version": "1.0.4",
  "author": {
    "name": "DoneJS Team",
    "email": "core@donejs.com",
    "url": "https://bitovi.com"
  },
  "bugs": {
    "url": "https://github.com/canjs/can-dom-mutate/issues"
  },
  "dependencies": {
    "can-globals": "^1.0.0"
  },
  "devDependencies": {
    "fixpack": "^2.3.1",
    "jshint": "^2.9.1",
    "steal": "^1.3.1",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.2.0",
    "testee": "^0.7.0"
  },
  "homepage": "https://github.com/canjs/can-dom-mutate",
  "keywords": [
    "canjs",
    "mutation",
    "mutationobserver",
    "observer"
  ],
  "license": "MIT",
  "main": "can-dom-mutate",
  "repository": {
    "type": "git",
